type Query {
    historicoPorPaciente(pacienteId: ID!): [HistoricoConsulta!]!
    historicoPorConsulta(consultaId: ID!): HistoricoConsulta
}

type HistoricoConsulta {
    id: ID!
    consultaId: ID!
    pacienteId: ID!
    medicoId: ID!
    dataHora: String!
    especialidade: String!
    status: String!
    observacoes: String
    dataCriacao: String!
    dataAtualizacao: String!
}
